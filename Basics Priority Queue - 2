import java.util.PriorityQueue;
import java.util.Comparator;

class Main {
    public static void main(String[] args) {
        PriorityQueue<Integer> pq = new PriorityQueue<>(Comparator.reverseOrder());
        
        pq.add(4);
        pq.add(6);
        pq.add(2);
        pq.add(10);
        pq.add(17);
        
        while(!pq.isEmpty()) {
            System.out.println(pq.peek());
            pq.remove();
        }
    }
}
